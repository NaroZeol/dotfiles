[user]
	name = NaroZeol
	email = narozeol@gmail.com
[init]
	defaultBranch = main
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[alias]
	graph = log --all --graph --pretty=format:'%C(auto)%h%C(auto)%d %s %C(dim white)(%aN, %ar)'
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
